generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model queue {
  element_identifier String    @id @db.Uuid
  time_inserted      DateTime? @db.Date
  payload            Json?     @db.Json

  @@index([time_inserted(sort: Asc)])
}

model users {
  id       Int     @id @default(autoincrement())
  username String?
  password String?
}
